    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const links = item.json.links || [];\n  const out = { ...item.json };\n\n  const norm = (url) => {\n    try { const u = new URL(url); u.hash=''; u.search=''; return u.toString(); }\n    catch { return url; }\n  };\n\n  const pick = (re) => {\n    const hit = links.find(h => typeof h === 'string' && re.test(h));\n    return hit ? norm(hit) : '';\n  };\n\n  out['Instagram link'] = pick(/https?:\\/\\/(www\\.)?instagram\\.com\\//i);\n  out['FaceBook link']  = pick(/https?:\\/\\/(www\\.)?facebook\\.com\\//i);\n  out['YouTube link']   = pick(/https?:\\/\\/(www\\.)?youtube\\.com\\/(channel|@|c\\/|user\\/)|https?:\\/\\/youtu\\.be\\//i);\n  out['TikTok link']    = pick(/https?:\\/\\/(www\\.)?tiktok\\.com\\//i);\n  out['Pinterest link'] = pick(/https?:\\/\\/(www\\.)?pinterest\\./i);\n  out['X link']         = pick(/https?:\\/\\/(www\\.)?(x\\.com|twitter\\.com)\\//i);\n\n  return { json: out };\n});"
      },
